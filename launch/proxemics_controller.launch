<launch>
  <node pkg="proxemics" type="proxemics_controller" name="proxemics_controller">
    <param name="robot_frame_id" value="/robot/base_link" type="str" />
    <param name="human_frame_id" value="/human/base_link" type="str" />
    <param name="goal_range_robot_to_human" value="1.5"   type="double" />
    <param name="goal_angle_robot_to_human" value="0.0"   type="double" />
    <param name="goal_angle_human_to_robot" value="0.0"   type="double" />
    <param name="min_speed_lin_x"           value="0.01"  type="double" />
    <param name="max_speed_lin_x"           value="1.5"   type="double" />
    <param name="min_speed_lin_y"           value="0.01"  type="double" />
    <param name="max_speed_lin_y"           value="1.5"   type="double" />
    <param name="min_speed_ang_z"           value="0.01"  type="double" />
    <param name="max_speed_ang_z"           value="360.0" type="double" />
    <param name="gain_p_lin_x"              value="1.0"   type="double" />
    <param name="gain_p_lin_y"              value="1.0"   type="double" />
    <param name="gain_p_ang_z"              value="1.0"   type="double" />
    <param name="gain_d_lin_x"              value="0.0"   type="double" />
    <param name="gain_d_lin_y"              value="0.0"   type="double" />
    <param name="gain_d_ang_z"              value="0.0"   type="double" />
  </node>

  <!--<node pkg="proxemics" type="proxemics_goal_state_estimation" name="proxemics_goal_state_estimation"/>-->
</launch>

